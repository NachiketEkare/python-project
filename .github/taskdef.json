{
  "family": "predict-service-task",
  "executionRoleArn": "arn:aws:iam::650080740886:role/predict-service-execution-role",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "fastapi",
      "image": "REPLACE_IMAGE",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "secrets": [
        {
          "name": "API_SECRET",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:650080740886:secret:predict-service-secret"
        }
      ]
    }
  ]
}
